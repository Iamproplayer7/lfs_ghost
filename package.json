{
  "type": "module",
  "name": "lfsoverlay",
  "version": "1.0.5",
  "main": "dist/src/index.js",
  "scripts": {
    "buildTS": "tsc",
    "dev": "npm run buildTS && node ./scripts/copyFiles.js && electron dist/src/index.js",
    "build": "npm run buildTS && node ./scripts/copyFiles.js && electron-builder"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "electron-overlay-window": "^3.3.0",
    "glob": "^11.0.2",
    "tsinsim": "^1.2.8"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "productName": "LFSGhost",
    "appId": "lfsghost",
    "files": [
      "build/*",
      "**/*",
      "!scripts/*",
      "!app/*",
      "!src/*",
      "!tsconfig.json",
      "!README.md",
      "!memoryjs.zip",
      "!memoryjs/*"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "icon": "build/iconp.ico"
    },
    "portable": {
      "artifactName": "LFSGhost.exe"
    },
    "directories": {
      "output": "builded"
    }
  }
}
